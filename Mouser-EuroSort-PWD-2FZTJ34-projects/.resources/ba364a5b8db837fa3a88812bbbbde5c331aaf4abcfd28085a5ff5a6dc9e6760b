SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_01' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_01 
WHERE TrackNo like :InVarTrack  

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_02' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_02 
WHERE TrackNo like :InVarTrack 

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_03' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_03 
WHERE TrackNo like :InVarTrack 

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_04' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_04 
WHERE TrackNo like :InVarTrack  

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_05' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_05  
WHERE TrackNo like :InVarTrack 

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_06' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_06 
WHERE TrackNo like :InVarTrack 

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_07' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_07 
WHERE TrackNo like :InVarTrack 

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_08' as DestinationLane , TimeStamp 
