SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_12' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_12 
WHERE TrackNo like :InVarTrack 

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_13' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_13 
WHERE TrackNo like :InVarTrack 

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_14' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_14 
WHERE TrackNo like :InVarTrack 

UNION ALL
SELECT IndexIDLane, TrackNo, RouteCode, 'Lane_15' as DestinationLane , TimeStamp 
FROM ShippingSorter_Lane_15 
WHERE TrackNo like :InVarTrack 
ORDER BY TimeStamp DESC;





